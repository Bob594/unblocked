{
  "name": "radon-games",
  "version": "5.0.0",
  "description": "An open-source unblocked games website built with the user first.",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsx bundle.ts && remix vite:build",
    "dev": "concurrently \"tsx bundle.ts --watch\" \"remix vite:dev\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./dist/server/index.js",
    "typecheck": "tsc",
    "format": "prisma format && prettier --write .",
    "postinstall": "prisma generate",
    "migrateData": "tsx ./src/util/migrateData.ts"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "@radon-games/emulatorjs": "github:Radon-Games/EmulatorJS",
    "@radon-games/unityloader": "*",
    "@remix-run/css-bundle": "^2.12.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "@ruffle-rs/ruffle": "*",
    "@titaniumnetwork-dev/ultraviolet": "^2.0.0",
    "cookie": "^0.6.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.16.3",
    "framer-motion": "^11.11.8",
    "fuzzy-search": "^3.2.1",
    "isbot": "^4.4.0",
    "mime-types": "^2.1.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "remix-utils": "^7.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cookie": "^0.6.0",
    "@types/fuzzy-search": "^2.1.5",
    "@types/mime-types": "^2.1.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "5.20.0",
    "react-unity-webgl": "^9.5.2",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
